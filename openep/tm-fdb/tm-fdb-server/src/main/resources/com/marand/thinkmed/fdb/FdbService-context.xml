<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="fdbService" class="com.marand.thinkmed.fdb.service.impl.FdbServiceImpl">
        <property name="host" value="${fdb.host}"/>
        <property name="port" value="${fdb.port}"/>
        <property name="username" value="${fdb.username}"/>
        <property name="password" value="${fdb.password}"/>
        <property name="restUri" value="${fdb.restUri}"/>
    </bean>

</beans>